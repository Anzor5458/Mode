local plushie = Instance.new("Model")
plushie.Name = "KardinHongPlushie"

local part = Instance.new("Part")
part.Size = Vector3.new(2, 2, 2)
part.Color = Color3.fromRGB(255, 0, 0) -- red color for the plushie
part.Anchored = true
part.Parent = plushie

local humanoid = Instance.new("Humanoid")
humanoid.Parent = plushie

plushie.Parent = workspace

